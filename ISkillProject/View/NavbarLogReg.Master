<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="NavbarLogReg.master.cs" Inherits="ISkillProject.View.NavbarLogReg" %>

<!DOCTYPE html>

<html>
<head runat="server">
    <title></title>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <link href="../Css/NavLogReg.css" rel="stylesheet" />
    <form id="form1" runat="server">
        <div>
            <div class="Header">
